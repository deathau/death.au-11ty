---
layout: base.njk
---

  <main class="card">
    {{ content | safe }}
    <aside class="post-info">
      <a href="{{ permalink }}">
      <i class="fa-solid fa-clock"></i>
      Last Updated
      <time class="dt-published" datetime="{{ omg.now.updatedISO | dateISOString }}">
          {{ omg.now.updatedISO | formatDate }}</time>
      </a>
    </aside> 
  </main>